{
  "version": 2,
  "builds": [
    {
      "src": "frontend/vite.config.ts",
      "use": "@vercel/vite"
    },
    {
      "src": "backend/app.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/generate-sql",
      "destination": "/backend/app.py"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/dist/index.html"
    }
  ]
}